{
  "cSpell.words": [
    "laball"
  ],
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    ".env": ".env, .env.*",
    "tsconfig.json": "tsconfig.*.json",
    ".gitattributes": ".gitattributes, .gitignore",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, pnpm-workspace.yaml, .nvmrc, bun.lockb"
  },
  "files.readonlyInclude": {
    ".env": true,
    "**/node_modules/**/*.*": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    ".pnpm-store": true,
  }
}